--
-- PostgreSQL database cluster dump
--

SET default_transaction_read_only = off;

SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;

--
-- Roles
--

CREATE ROLE "ec2-user";
ALTER ROLE "ec2-user" WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN NOREPLICATION PASSWORD 'md541eda8140bb2aeb3f557fc4a07d2fc8b';
CREATE ROLE emilyodea;
ALTER ROLE emilyodea WITH NOSUPERUSER INHERIT NOCREATEROLE NOCREATEDB LOGIN NOREPLICATION PASSWORD 'md52a21dadcf2ff1b2ba1b91b38c11b967f';
CREATE ROLE postgres;
ALTER ROLE postgres WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN REPLICATION PASSWORD 'md5278500d7ad8049c7e3526b906b6e16f0';
CREATE ROLE root;
ALTER ROLE root WITH NOSUPERUSER INHERIT NOCREATEROLE NOCREATEDB LOGIN NOREPLICATION PASSWORD 'md53195d82b8113a8f5f8f8573be3b1f4ea';






--
-- Database creation
--

CREATE DATABASE cwa_cloud_development WITH TEMPLATE = template0 OWNER = "ec2-user";
CREATE DATABASE cwa_db WITH TEMPLATE = template0 OWNER = postgres;
REVOKE ALL ON DATABASE cwa_db FROM PUBLIC;
REVOKE ALL ON DATABASE cwa_db FROM postgres;
GRANT ALL ON DATABASE cwa_db TO postgres;
GRANT CONNECT,TEMPORARY ON DATABASE cwa_db TO PUBLIC;
GRANT ALL ON DATABASE cwa_db TO emilyodea;
GRANT ALL ON DATABASE cwa_db TO root;
CREATE DATABASE root WITH TEMPLATE = template0 OWNER = postgres;
REVOKE ALL ON DATABASE template1 FROM PUBLIC;
REVOKE ALL ON DATABASE template1 FROM postgres;
GRANT ALL ON DATABASE template1 TO postgres;
GRANT CONNECT ON DATABASE template1 TO PUBLIC;


\connect cwa_cloud_development

SET default_transaction_read_only = off;

--
-- PostgreSQL database dump
--

SET statement_timeout = 0;
SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;
SET check_function_bodies = false;
SET client_min_messages = warning;

--
-- Name: plpgsql; Type: EXTENSION; Schema: -; Owner: 
--

CREATE EXTENSION IF NOT EXISTS plpgsql WITH SCHEMA pg_catalog;


--
-- Name: EXTENSION plpgsql; Type: COMMENT; Schema: -; Owner: 
--

COMMENT ON EXTENSION plpgsql IS 'PL/pgSQL procedural language';


--
-- Name: public; Type: ACL; Schema: -; Owner: postgres
--

REVOKE ALL ON SCHEMA public FROM PUBLIC;
REVOKE ALL ON SCHEMA public FROM postgres;
GRANT ALL ON SCHEMA public TO postgres;
GRANT ALL ON SCHEMA public TO PUBLIC;


